var __translate = mat4.create();
var __rotateX = mat4.create();
var __rotateY = mat4.create();
var __rotateZ = mat4.create();
var __scale = mat4.create();
mat4.translate(__translate, __translate, vec3(0.0, 0.0, 0.0));
mat4.rotateX(__rotateX, __rotateX, 0.0);
mat4.rotateY(__rotateY, __rotateY, 0.0);
mat4.rotateZ(__rotateZ, __rotateZ, 0.0);
mat4.scale(__scale, __scale, vec3(0.01, 0.01, 0.01));

var __identity = (
    __translate,
    __rotateX,
    __rotateY,
    __rotateZ,
    __scale
);

