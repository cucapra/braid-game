                    vec3(0.0, 2.5, 5.0),
                    # Local Transform,
                    __identity,
                    # Render Object,
                    (teapot, __wood, "phong"),
                    # Collider
                    ("sphere", vec2(0.5, 0.5), __identity),
                    # Light
                    array{Light}(),
                    # Triggers
                    array{Trigger}()
                )
            ),
